h2.display-4 User Admin
.table-responsive-sm
  table.table.table-striped.table-hover.table-sm
    caption List of users.
    thead
      tr
        th(scope="col") #
        th(scope="col") User Name
    tbody
      tr(ng-repeat="user in users")
        td(scope="row") {{$index + 1}}
        td {{user.firstName + ' ' + user.lastName}}
